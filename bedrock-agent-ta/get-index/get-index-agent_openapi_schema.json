{
    "openapi": "3.0.0",
        "info": {
            "title": "Get betIndex constituents api",
            "version": "1.0.0",
            "description": "API to fetch the list of a given index"
        },
        "paths": {
            "/get-index/{indexName}": {
                "get": {
                    "summary": "Get list of stock symbols in index",
                    "description": "Based on provided index, return list of stock symbols in the index",
                    "operationId": "getIndex",
                    "parameters": [{
                        "name": "IndexName",
                        "in": "path",
                        "description": "Index Name",
                        "required": true,
                        "schema": {
                            "type": "string"
                        }
                    }],
                    "responses": {
                        "200": {
                            "description": "Get Index stock list",
                            "content": {
                                "application/json": {
                                    "schema": {
                                        "type": "array",
                                        "items": {
                                            "type": "object",
                                            "properties": {
                                                "indexName": {
                                                    "type": "String",
                                                    "description": "This is the name of the Index for which stocks where requested"
                                                },
                                                "stocksList": {
                                                    "type": "array",
                                                    "description": "List of stock symbols in the index"
                                                }
                                            }
                                        }
                                    }
                                }
                            }
                        }
                    }
                }
            },
            
            
        }    
    }
    